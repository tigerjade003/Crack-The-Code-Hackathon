import datetime


#example data = ("apple", "banana")

class Block:  
    def __int__(self, index, previous_hash, data, timestamp):
        #index tells position or number value assigned to block
        self.index = index
        self.previous_hash = previous_hash
        self.data = data
        self.timestamp = timestamp
        
class Blockchain:
    def __init__(self):
        self.chain = []


















